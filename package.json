{
  "dependencies": {
    "@uirouter/angularjs": "^1.0.4",
    "angular": "^1.6.4",
    "systemjs": "^0.20.14"
  },
  "devDependencies": {
    "@types/angular": "^1.6.20",
    "@types/angular-mocks": "^1.5.10",
    "@types/angular-ui-router": "^1.1.37",
    "@types/jasmine": "^2.5.53",
    "angular-mocks": "^1.6.4",
    "babel-polyfill": "^6.23.0",
    "concurrently": "^3.5.0",
    "express": "^4.15.3",
    "express-cors": "0.0.3",
    "jasmine-core": "^2.6.4",
    "karma": "^1.7.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "lite-server": "^2.3.0",
    "phantomjs-prebuilt": "^2.1.14",
    "pug-cli": "^1.0.0-alpha6",
    "ts-node": "^3.1.0",
    "typescript": "^2.3.4"
  },
  "scripts": {
    "compile": "concurrently \"npm run compile:pug\" \"npm run compile:typescript\"",
    "compile:pug": "pug src/",
    "compile:typescript": "tsc -p src/",
    "serve": "lite-server -c=bs-config.json",
    "start": "concurrently \"npm run watch\" \"npm run serve\"",
    "stub": "ts-node -P resources/server resources/server/index.ts",
    "test": "karma start",
    "watch": "concurrently \"npm run watch:pug\" \"npm run watch:typescript\"",
    "watch:pug": "npm run compile:pug -- -P -w",
    "watch:typescript": "npm run compile:typescript -- -w"
  }
}
